(ert-deftest test-tcel-call-gen ()
  (should (equal 1 (tcel-call-gen (tcel-gen-pure 1) nil nil))))


(ert-deftest test-tcel-rand-range ()
  (let ((rnd (tcel-random 42))	(result (list)))
    (should (equal '(42 13 29 23 26 38 48 42 47 30)
		   (dotimes (_ 10 result)
		     (setq result (cons (tcel-rand-range rnd 10 50) result)))))))
    
	     
    
	     
